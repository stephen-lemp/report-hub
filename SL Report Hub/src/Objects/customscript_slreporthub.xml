<suitelet scriptid="customscript_slreporthub">
  <description>Runs reports for user based on roles and permissions defined on Report Hub Config
    records.</description>
  <isinactive>F</isinactive>
  <name>SL Report Hub</name>
  <notifyadmins>F</notifyadmins>
  <notifyemails></notifyemails>
  <notifyowner>T</notifyowner>
  <notifyuser>F</notifyuser>
  <scriptfile>[/SuiteScripts/sl/reportHub/main.js]</scriptfile>
  <scriptcustomfields>
    <scriptcustomfield scriptid="custscript_slrh_suitelet_reportconfig">
      <accesslevel>2</accesslevel>
      <applyformatting>F</applyformatting>
      <checkspelling>F</checkspelling>
      <defaultchecked>F</defaultchecked>
      <defaultselection></defaultselection>
      <defaultvalue></defaultvalue>
      <description>Select the report to run for this deployment. If blank, page will return a list
        of all available reports.</description>
      <displayheight></displayheight>
      <displaytype>NORMAL</displaytype>
      <displaywidth></displaywidth>
      <dynamicdefault></dynamicdefault>
      <fieldtype>SELECT</fieldtype>
      <help>Select the report to run for this deployment. If blank, page will return a list of all
        available reports.</help>
      <isformula>F</isformula>
      <ismandatory>F</ismandatory>
      <label>Report Config</label>
      <linktext></linktext>
      <maxlength></maxlength>
      <maxvalue></maxvalue>
      <minvalue></minvalue>
      <onparentdelete>NO_ACTION</onparentdelete>
      <searchlevel>2</searchlevel>
      <selectrecordtype>[scriptid=customrecord_sl_reporthubconfig]</selectrecordtype>
      <setting></setting>
      <storevalue>T</storevalue>
    </scriptcustomfield>
  </scriptcustomfields>
  <scriptdeployments>
    <scriptdeployment scriptid="customdeploy_slreporthub">
      <allemployees>F</allemployees>
      <allpartners>F</allpartners>
      <allroles>F</allroles>
      <audslctrole>ADMINISTRATOR</audslctrole>
      <custscript_slrh_suitelet_reportconfig></custscript_slrh_suitelet_reportconfig>
      <eventtype></eventtype>
      <isdeployed>T</isdeployed>
      <isonline>F</isonline>
      <loglevel>ERROR</loglevel>
      <runasrole></runasrole>
      <status>RELEASED</status>
      <title>SL Report Hub - List</title>
      <links>
        <link>
          <linkcategory>BASICLISTSSEARCH</linkcategory>
          <linklabel>Report Hub - List</linklabel>
        </link>
      </links>
    </scriptdeployment>
  </scriptdeployments>
</suitelet>